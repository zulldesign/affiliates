<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@System.Web.Optimization.BundleTable.Bundles.ResolveBundleUrl("~/Content/css")" rel="stylesheet" type="text/css" />
    <link href="@System.Web.Optimization.BundleTable.Bundles.ResolveBundleUrl("~/Content/themes/base/css")" rel="stylesheet" type="text/css" />
    <script src="@System.Web.Optimization.BundleTable.Bundles.ResolveBundleUrl("~/Scripts/js")"></script>
    <meta name="viewport" content="width=device-width" />
</head>
<body>
    <header>
        <a href="https://github.com/dotnetcurry/FunWithSignalR">
            <img style="position: absolute; top: 0; right: 0; border: 0;" src="https://s3.amazonaws.com/github/ribbons/forkme_right_darkblue_121621.png"
                alt="Fork me on GitHub">
        </a>
        @Html.Partial("_ViewSwitcher")
        <div class="content-wrapper">
            <div class="float-left">
                <p class="site-title">
                    <a href="/">Fun with SignalR</a></p>
            </div>
        </div>
    </header>
    <div>
        @RenderBody()
    </div>
</body>
</html>
